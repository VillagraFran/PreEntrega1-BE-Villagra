<div class="container">
    {{#each cart}}
        <div class="card">
            <h5 class="card-header">{{this.product}}</h5>
            <div class="card-body">
                <p class="card-text">{{this.quantity}}</p>
            </div>
        </div>
    {{/each}}
</div>

